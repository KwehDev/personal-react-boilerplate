{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^5.12.1",
    "bulma-checkradio": "^1.1.1",
    "bulmaswatch": "^0.7.5",
    "classnames": "^2.2.6",
    "faunadb": "^2.11.1",
    "i18next": "^19.1.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-chained-backend": "^2.0.1",
    "i18next-localstorage-backend": "^3.0.0",
    "i18next-multiload-backend-adapter": "^0.1.1",
    "i18next-xhr-backend": "^3.2.2",
    "logrocket": "^1.0.6",
    "logrocket-react": "^4.0.1",
    "rbx": "^2.2.0",
    "react": "^16.12.0",
    "react-awesome-reveal": "^2.4.2",
    "react-countup": "^4.3.3",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.3.1",
    "react-icons": "^3.9.0",
    "react-loadable": "^5.5.0",
    "react-netlify-identity": "^0.2.3",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "react-toastify": "^5.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "customize-cra": "^0.9.1",
    "dotenv-webpack": "^1.7.0",
    "encoding": "^0.1.12",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "netlify-lambda": "^1.6.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "sw-precache-cra": "^1.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start": "run-p start:**",
    "start:app": "react-app-rewired start",
    "start:lambda": "netlify-lambda serve -c ./config/webpack.js src/lambda",
    "build": "run-p build:**",
    "build:app": "react-app-rewired build && sw-precache-cra --config ./config/sw-config.js",
    "build:lambda": "netlify-lambda build -c ./config/webpack.js src/lambda",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "nuke-modules": "rm -rf node_modules/ && yarn install",
    "precompile-dark-theme": "node-sass src/assets/test.scss public/dark.min.css --output-style=compressed --source-map=true"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal"
            ],
            [
              "unknown",
              "parent",
              "sibling"
            ],
            "index"
          ],
          "alphabetize": {
            "order": "asc"
          },
          "newlines-between": "always"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
    "*.js": "eslint --fix"
  }
}
